     </server>
  </servers>
  <mirrors>
    <mirror>
      <!--This sends everything else to /public -->
      <id>central-redhat</id>
      <mirrorOf>*</mirrorOf>
      <url>https://maven.repository.redhat.com/ga/</url>
    </mirror>
    <mirror>
      <!--This sends everything else to /public -->
      <id>central-maven</id>
      <mirrorOf>*</mirrorOf>
      <url>https://repo1.maven.org/maven2/</url>
    </mirror>

  </mirrors>
  <profiles>
    <profile>
      <id>central</id>
      <!--Enable snapshots for the built in central repo to direct -->
      <!--all requests to central via the mirror -->
      <repositories>
        <repository>
          <id>central</id>
          <url>http://central</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
      </repositories>
     <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>http://central</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <!--make the profile active all the time -->
    <activeProfile>central</activeProfile>
  </activeProfiles>
